BW-UC-Connect DEFINITIONS ::= BEGIN
    IMPORTS
      DateAndTime, DisplayString
        FROM SNMPv2-TC
      MODULE-COMPLIANCE, OBJECT-GROUP          
        FROM SNMPv2-CONF
      enterprises, MODULE-IDENTITY, OBJECT-TYPE, Gauge32, Counter32, Integer32, Unsigned32
        FROM SNMPv2-SMI;

  broadsoft MODULE-IDENTITY
    LAST-UPDATED  "200906020000Z"
    ORGANIZATION  "Broadsoft, Inc"
    CONTACT-INFO  "Broadsoft, Inc
220 Perry Parkway
Gaithersburg, MD  20877

301-977-9440"
    DESCRIPTION
      "This is the MIB for the BroadWorks UC-Connect application."
    REVISION    "200906020000Z"
      DESCRIPTION     "Added Session Audit counters."
    REVISION    "200903300000Z"
      DESCRIPTION     "Creation."
    ::= { enterprises 6431 }

  broadworks OBJECT IDENTIFIER
    ::= { broadsoft 1 }

  ucconnect OBJECT IDENTIFIER
    ::= { broadworks 15 }

  protocol OBJECT IDENTIFIER
		::= { ucconnect 1 }

  capModule OBJECT IDENTIFIER
		::= { ucconnect 2 }

  locationApiModule OBJECT IDENTIFIER
    ::= { ucconnect 3 }

  cstaModule OBJECT IDENTIFIER
    ::= { ucconnect 4 }

  sessionAudit  OBJECT IDENTIFIER
    ::= { ucconnect 5 }

  internalModule OBJECT IDENTIFIER
    ::= { ucconnect 6 }

  overloadModule OBJECT IDENTIFIER
    ::= { ucconnect 7 }

  capStats OBJECT IDENTIFIER
    ::= { capModule 1 }
   
  locationApiStats OBJECT IDENTIFIER
    ::= { locationApiModule 1 }

  cstaStats OBJECT IDENTIFIER
    ::= { cstaModule 1 }

  internalStats OBJECT IDENTIFIER
    ::= { internalModule 1 }

  overloadStats OBJECT IDENTIFIER
    ::= { overloadModule 1 }

  bwUCConnectMibConformance OBJECT IDENTIFIER
    ::= { ucconnect 100 }

