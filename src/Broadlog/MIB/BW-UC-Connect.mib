-- MODULE - Internal Stats
  bwUCConnectInternalQueueResets OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION "When set to a value other then 0, all the internal queue statistic stored under bwUCConnectInternalQueueTable are reset to 0."
      ::= { internalStats 1 }

  bwUCConnectInternalQueueTable OBJECT-TYPE
    SYNTAX SEQUENCE OF BwUCConnectInternalQueueEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
      "This table contains statistics about the BroadWorks internal queues."
    ::= { internalStats 2 }

  bwUCConnectInternalQueueEntry OBJECT-TYPE
    SYNTAX BwUCConnectInternalQueueEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
      "This row contains the statistics for a given internal queue in the system"
    INDEX { bwUCConnectInternalQueueIndex }
    ::= { bwUCConnectInternalQueueTable 1 }

    BwUCConnectInternalQueueEntry ::= SEQUENCE 
    {
      bwUCConnectInternalQueueIndex Unsigned32,
      bwUCConnectInternalQueueName DisplayString,
      bwUCConnectInternalQueueSize Counter32,
      bwUCConnectInternalQueueTimeAvg Gauge32,
      bwUCConnectInternalQueueTimeMin Gauge32,
      bwUCConnectInternalQueueTimeMax Gauge32,
      bwUCConnectInternalQueueLengthCurrent Gauge32,
      bwUCConnectInternalQueueLengthAvg Gauge32,
      bwUCConnectInternalQueueLengthMax Gauge32,
      bwUCConnectInternalQueueTimeMaxTimestampMSB Gauge32,
      bwUCConnectInternalQueueLengthMaxTimestampMSB Gauge32,
      bwUCConnectInternalQueueLengthMaxTimestampLSB Gauge32,
      bwUCConnectInternalQueueTimeMaxTimestampLSB Gauge32
    }

  bwUCConnectInternalQueueIndex OBJECT-TYPE
    SYNTAX Unsigned32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
      "This integer represents an index to an internal queue in the application."
    ::= { bwUCConnectInternalQueueEntry 1 }

  bwUCConnectInternalQueueName OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
      "This string represents the name of an internal queue."
    ::= { bwUCConnectInternalQueueEntry 2 }

  bwUCConnectInternalQueueSize OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
      "The number of sample elements that the average, minimum and maximum holding time were calculated from."
    ::= { bwUCConnectInternalQueueEntry 3 }

  bwUCConnectInternalQueueTimeAvg OBJECT-TYPE
    SYNTAX Gauge32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
      "The average queue holding time. Note that this value is in 1/1000 of millisec, i.e. divide by 1000 to get the value in milliseconds."
    ::= { bwUCConnectInternalQueueEntry 4 }

  bwUCConnectInternalQueueTimeMin OBJECT-TYPE
    SYNTAX Gauge32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
      "The smallest queue sample holding time."
    ::= { bwUCConnectInternalQueueEntry 5 }

  bwUCConnectInternalQueueTimeMax OBJECT-TYPE
    SYNTAX Gauge32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
      "The biggest queue sample holding time."
    ::= { bwUCConnectInternalQueueEntry 6 }

  bwUCConnectInternalQueueLengthCurrent OBJECT-TYPE
    SYNTAX Gauge32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
      "The number of elements currently hold in the queue."
    ::= { bwUCConnectInternalQueueEntry 7 }

  bwUCConnectInternalQueueLengthAvg OBJECT-TYPE
    SYNTAX Gauge32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
      "The average number of elements simultaneously held in the queue multiple by 1000. Divide by 1000 to get the value in units."
    ::= { bwUCConnectInternalQueueEntry 8 }

  bwUCConnectInternalQueueLengthMax OBJECT-TYPE
    SYNTAX Gauge32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
      "The maximum number of elements simultaneously held in the queue multiple by 1000. Divide by 1000 to get the value in units."
    ::= { bwUCConnectInternalQueueEntry 9 }

  bwUCConnectInternalQueueTimeMaxTimestampMSB OBJECT-TYPE
    SYNTAX Gauge32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
      "The most significant part of the timestamp (4 most significant bytes) of when bwUCConnectInternalQueueTimeMax was obtained."
    ::= { bwUCConnectInternalQueueEntry 10 }

  bwUCConnectInternalQueueLengthMaxTimestampMSB  OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS    read-only
    STATUS      current
                DESCRIPTION
                                "The most significant part of the timestamp (4 most significant bytes) of when bwUCConnectInternalQueueLengthMax was obtained."
                ::=  {  bwUCConnectInternalQueueEntry  11 }
                
  bwUCConnectInternalQueueLengthMaxTimestampLSB  OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS    read-only
    STATUS      current
                DESCRIPTION
                                "The least significant part of the timestamp (4 least significant bytes) of when bwUCConnectInternalQueueLengthMax was obtained."
    ::=  {  bwUCConnectInternalQueueEntry  12 }    
                
  bwUCConnectInternalQueueTimeMaxTimestampLSB  OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS    read-only
    STATUS      current
                DESCRIPTION
                                "The least significant part of the timestamp (4 least significant bytes) of when bwUCConnectInternalQueueTimeMax was obtained."
                ::=  {  bwUCConnectInternalQueueEntry  13 }

