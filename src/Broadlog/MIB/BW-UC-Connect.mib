-- PROTOCOL :: BCCT Protocol ...............................

	bwUCConnectCommonCommStats OBJECT IDENTIFIER
		::=  { protocol 2 }

	bwUCConnectCommonCommStatsTable	OBJECT-TYPE
		SYNTAX		SEQUENCE  OF  BwUCConnectCommonCommStatsEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"This table contains counters for BCCT for the UC-Connect Application."
		::=  { bwUCConnectCommonCommStats 1 }

	bwUCConnectCommonCommStatsEntry	OBJECT-TYPE
		SYNTAX		BwUCConnectCommonCommStatsEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Definition of the entries for bwUCConnectCommonCommStatsTable."
		INDEX		{  bwUCConnectCommonCommStatsIndex  }
		::=  { bwUCConnectCommonCommStatsTable 1 }

	BwUCConnectCommonCommStatsEntry  ::=  SEQUENCE {
		bwUCConnectCommonCommStatsIndex  Unsigned32,
		bwUCConnectCommonCommHost  DisplayString,
		bwUCConnectCommonCommInterface  DisplayString,
		bwUCConnectCommonCommProtocol  DisplayString,
		bwUCConnectCommonCommAcceptedOutboundConnections  Counter32,
		bwUCConnectCommonCommAcceptedInboundConnections  Counter32,
		bwUCConnectCommonCommRejectedOutboundConnections  Counter32,
		bwUCConnectCommonCommRejectedInboundConnections  Counter32,
		bwUCConnectCommonCommOutputMessagesProcessed  Counter32,
		bwUCConnectCommonCommInputMessagesProcessed  Counter32,
		bwUCConnectCommonCommOutputCommunicationErrors  Counter32,
		bwUCConnectCommonCommInputCommunicationErrors  Counter32
		}

	bwUCConnectCommonCommStatsIndex	OBJECT-TYPE
		SYNTAX			Unsigned32
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION
				"This integer represents an index in table bwUCConnectCommonCommStatsTable. The index represents a connection for a specific Host-Interfacece-Protocol combination."
		::=  {  bwUCConnectCommonCommStatsEntry  1  }

	bwUCConnectCommonCommHost	OBJECT-TYPE
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION
				"The address of a host connected to the BroadWorks Common Communication Protocol."
		::=  {  bwUCConnectCommonCommStatsEntry  2  }


	bwUCConnectCommonCommInterface	OBJECT-TYPE
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION
				"The IP address of a local interface that a protocol is bound to."
		::=  {  bwUCConnectCommonCommStatsEntry  3  }

